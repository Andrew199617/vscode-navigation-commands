{
  "name": "vscode-navigation-commands",
  "displayName": "Keyboard Navigation Commands",
  "version": "1.1.3",
  "icon": "LGDLogo.png",
  "publisher": "learn-game-development",
  "description": "Commands that you can bind to a keyboard shortcut to help with navigating a file. For example, moving to the next or previous Paragraph, word, WORD, line, character, or more.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrew199617/vscode-navigation-commands"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest --config ./tests/jest.config.js"
  },
  "categories": [
    "Programming Languages",
    "Other",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
  "onLanguage:csharp",
  "onLanguage:c",
  "onLanguage:cpp"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lgd.goToNextParagraph",
        "title": "Go to next chuck of code separated by a new line.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToLastParagraph",
        "title": "Go to last chuck of code separated by a new line.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToNextParenthesis",
        "title": "Go to next open parenthesis.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToLastParenthesis",
        "title": "Go to last open parenthesis.",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideNextParenthesis",
        "title": "Go to next parenthesis and highlight the inside of it.",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideLastParenthesis",
        "title": "Go to last parenthesis and highlight the inside of it.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToAssignment",
        "title": "Go to the beginning of an assignment. Searches for first letter after an equals sign.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToNextBracket",
        "title": "Go to next closing bracket. Place cursor behind the }",
        "category": "LGD"
      },
      {
        "command": "lgd.goToLastBracket",
        "title": "Go to last closing bracket. Place cursor behind the }",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToNextOpenBracket",
        "title": "Move to next opening bracket. Place cursor after the {",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToLastOpenBracket",
        "title": "Move to last opening bracket. Place cursor after the {",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideNextBracket",
        "title": "Highlight inside next bracket. Select text between { and }",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideLastBracket",
        "title": "Highlight inside last bracket. Select text between { and }",
        "category": "LGD"
      },
      {
        "command": "lgd.goToNextMethod",
        "title": "Go to next method in class, object, or module.",
        "category": "LGD"
      },
      {
        "command": "lgd.goToLastMethod",
        "title": "Go to last method in class, object, or module.",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToNextStringChar",
        "title": "Move To Next String Character",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToLastStringChar",
        "title": "Move To Last String Character",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideNextStringChar",
        "title": "Highlight Inside Next String Character",
        "category": "LGD"
      },
      {
        "command": "lgd.highlightInsideLastStringChar",
        "title": "Highlight Inside Last String Character",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToNextClosingParenthesis",
        "title": "Move To Next Closing Parenthesis",
        "category": "LGD"
      },
      {
        "command": "lgd.moveToLastClosingParenthesis",
        "title": "Move To Last Closing Parenthesis",
        "category": "LGD"
      },
      {
        "command": "lgd.removeUnused",
        "title": "Remove Unused Usings (C#)",
        "category": "LGD"
      },
      {
        "command": "lgd.openHeaderOrSourceBeside",
        "title": "Open Header/Source Beside (Preview)",
        "category": "LGD"
      }
    ]
  },
  "engines": {
    "node": "20.18.0",
    "vscode": "^1.1.0"
  },
  "keywords": [
    "lgd",
    "navigation",
    "vscode",
    "extension",
    "vim",
    "Go to next paragraph",
    "Go to last paragraph",
    "Go to next word",
    "Go to last word",
    "Go to next WORD",
    "Go to last WORD",
    "Go to next line",
    "Go to last line",
    "csharp",
    "javascript",
    "typescript",
    "Go to last string character",
    "Highlight inside next string character",
    "Highlight inside last string character"
  ],
  "author": "Learn Game Development",
  "license": "MIT",
  "dependencies": {
    "@mavega/oloo": "^2.3.1"
  },
  "devDependencies": {
    "vscode": "^1.1.34"
  }
}