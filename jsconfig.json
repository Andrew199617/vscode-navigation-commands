{
  "compilerOptions": {
    "checkJs": false,
    "target": "ES2022",
    "jsx": "react-jsx",
    "module": "ES2022",
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
  },
  "exclude": [
    "node_modules"
  ]
}